import "Unity";

const birb = Unity.Fetch("Birb");

birb.Update = () => {
	if (Unity.GetButtonDown("Fire1")) {
		birb.Rigidbody2D.AddForce(0, 10, "impulse");
	}
};

birb.FixedUpdate = () => {
	birb.Rigidbody2D.Rotation += 1;
};

birb.OnCollisionEnter2D = other => {
	if (other.tag == "wall") {
		birb.GameObject.Destroy();
		print "You lose!";
	}
};